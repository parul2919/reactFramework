{
  "name": "react-base",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@testing-library/user-event": "^13.5.0",
    "commitlint": "^17.0.3",
    "moment": "^2.29.3",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^1.4.1",
    "react-scripts": "5.0.1",
    "react-webcam": "^7.0.1",
    "redux-mock-store": "^1.5.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "env-cmd -f .env.development react-scripts start",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging npm run-script build",
    "build:production": "env-cmd -f .env.production npm run-script build",
    "test": "env-cmd -f .env.development jest --coverage",
    "test:staging": "env-cmd -f .env.staging jest --coverage",
    "test:production": "env-cmd -f .env.production jest --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint  \"src/**/*.{js, jsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js, jsx}\"",
    "prepare": "husky install"
  },
  "roots": [
    "<rootDir>/src"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "husky": {
    "hooks": {
      "prepare": "npm run build",
      "applypatch-msg": "echo \"[Husky] applypatch-msg\"",
      "pre-applypatch": "echo \"[Husky] pre-applypatch\"",
      "post-applypatch": "echo \"[Husky] post-applypatch\"",

      "pre-commit": "lint-staged ",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test"
    }
  },
  "commitlint": {
    "extends": [
      "./@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:fix"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "keywords": [
    "react",
    "eslint",
    "prittier",
    "sonar",
    "husky",
    "javascript",
    "jsx",
    "hooks",
    "material ui",
    "JDK11",
    "sonar9"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.4",
    "airbnb": "0.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-transform-css": "^4.0.1",
    "prettier": "^2.7.1",
    "react-hook-form": "^7.33.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.5"
  }
}
